{
    "name": "amelia/money",
    "description": "A library for working with the Open Exchange Rates API in PHP",
    "keywords": ["open", "exchange", "rates", "openexchangerates", "currency", "exchange", "money"],
    "type": "library",
    "require": {
        "guzzlehttp/guzzle": "5.*",
        "nesbot/carbon": "1.*",
        "php": ">= 5.4",
        "ext-openssl": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Amelia Ikeda",
            "email": "amelia@dorks.io"
        }
    ],
    "autoload": {
        "psr-4": {
            "Amelia\\Money\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Amelia\\Money\\": "test/"
        }
    },
    "minimum-stability": "stable"
}
